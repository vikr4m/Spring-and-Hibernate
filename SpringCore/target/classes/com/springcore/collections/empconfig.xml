<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/context"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans
 		http://www.springframework.org/schema/beans/spring-beans.xsd
 		http://www.springframework.org/context
 		http://www.springframework.org/context/spring-context.xsd">
	
	<!-- To inject collections (set,map,list,properties) -->
	<bean class="com.springcore.collections.Emp" name="emp1">
		<property name="name" value="Vikram"/>
		
		<property name="phones">
			<list>
				<value>123456</value>
				<value>789456</value>
				<value>456159</value>
			</list>
		</property>
		
		<property name="addresses">
			<set>
				<value>Anand</value>
				<value>Ahmedabad</value>
				<value>Vadodara</value>
			</set>
		</property>
		
		<property name="courses">
			<map>
				<entry key="java" value="2 month"/>
				<entry key="python" value="1 month"/>
				<entry key="android" value="4 month"/>
			</map>
		</property>
	</bean>
	
</beans>